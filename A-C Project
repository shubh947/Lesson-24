<body>
    <h2>Set of random numbers</h2>
    <p id="number"></p>
    <script>
        var text = "";
        var i = 0;
        while (i < 100) {
            text += "<br>The number is" + i;
            i++;
        }
        document.getElementById("number").innerHTML = text
    </script>
